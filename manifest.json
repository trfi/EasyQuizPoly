{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy Quiz Poly",
  "description": "Tiện ích hỗ trợ sinh viên làm bài Quiz trường Cao đẳng FPT Polytechnic",
  "background" : {
    "scripts" : ["background.js"],
    "persistent" : false
  },
  "content_scripts": [
    {
      "matches": ["http://*.poly.edu.vn/*&sequence=*"],
      "run_at": "document_end",
      "js": ["lmsAutoAswer.js"]
    },
    {
      "matches": ["http://*.poly.edu.vn/*ilexsubmissionfilegui*"],
      "js": ["unlimitUploadFshare.js"]
    }
  ],
  "version": "1.2.0",
  "author": "fb.com/hi.trfi",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Easy Quiz Poly"
  },
  "permissions": [
    "*://*.poly.edu.vn/*",
    "declarativeContent",
    "contextMenus",
    "storage"
  ]
}