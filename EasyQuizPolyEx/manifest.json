{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Easy Quiz Poly",
  "description": "Tiện ích hỗ trợ sinh viên làm bài Quiz trường Cao đẳng FPT Polytechnic",
  "background" : {
    "scripts" : ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.poly.edu.vn/*&sequence=*"],
      "exclude_matches": ["*://*.poly.edu.vn/*cmd=outQuestionSummary*", "*://*.poly.edu.vn/*cmd=showPasswordForm*", "*://*.poly.edu.vn/*cmd=finishTest*"],
      "run_at": "document_end",
      "js": ["lms_script.js"]
    },
    {
      "matches": ["*://*.poly.edu.vn/*target=tst_*", "*://*.poly.edu.vn/*cmdClass=ilobjtestgui*", "*://*.poly.edu.vn/*cmd=infoScreen*"],
      "run_at": "document_end",
      "js": ["lms_start.js"]
    },
    {
      "matches": ["*://*.poly.edu.vn/*ilexsubmissionfilegui*"],
      "js": ["unlimitUploadFshare.js"]
    }
  ],
  "version": "1.8.0",
  "author": "fb.com/hi.trfi",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "Easy Quiz Poly"
  },
  "web_accessible_resources": [
    "cms_data.json"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "*://*.poly.edu.vn/*",
    "https://*.facebook.com/*",
    "declarativeContent",
    "contextMenus",
    "storage"
  ]
}